{
    "name": "kt-advance",
    "displayName": "kt-advance",
    "description": "KT Advance",
    "version": "2.7.2",
    "publisher": "compartia",
    "engines": {
        "vscode": "^1.21.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/compartia/kt-ls.git"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:c"
    ],
    "main": "./out/src/extension",
    "files": [
        "./out/kt-ls.jar"
    ],
    "contributes": {
        "languages": [
            {
                "id": "c",
                "extensions": [
                    ".c"
                ],
                "aliases": [
                    "c"
                ]
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "vscode-languageclient": "^4.0.0",
        "find-java-home": "0.1.4",
        "path-exists": "^3.0.0",
        "expand-home-dir": "^0.0.3",
        "glob": "^7.1.1",
        "portfinder": "^1.0.13"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "@types/node": "^6.0.40",
        "@types/glob": "5.0.30",
        "@types/mocha": "^2.2.42",
        "tslint": "^5.8.0",
        "vscode": "^1.1.6"
    }
}
